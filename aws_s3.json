{
	"Version": "2012-10-17",
	"Statement": [
		{
			"Effect": "Allow",
			"Action": [
				"s3:GetBucketLocation",
				"s3:ListAllMyBuckets"
			],
			"Resource": "arn:aws:s3:::*"
		},
		{
			"Effect": "Allow",
			"Action": "s3:*",
			"Resource": [
				"arn:aws:s3:::bebv-wpbackups",
				"arn:aws:s3:::bebv-wpbackups/wpvividbackuppro",
				"arn:aws:s3:::bebv-wpbackups/wpvividbackuppro/ddomain_ch",
				"arn:aws:s3:::bebv-wpbackups/wpvividbackuppro/ddomain_ch/*"
			]
		}
	]
}